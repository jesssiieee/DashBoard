<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.monitor.node.mapper.TGWMapper">
    <select id="selectTgwList">

        SELECT
            `id`
             , `area_name`
             , `forward_group_name`
             , `name`
             , `depth`
        FROM
            `map_info`

    </select>
</mapper>